<%- include("../partials/header")%>

 

<% teams.forEach(t=> { %>
    <div class="eachTeam">
        <div class="sprite">
                <div id="sprite">
                    <div class="details">
                        <p height="30px" width="30px"><a href="/teams/<%=t._id%>">
                                <img id="details"
                                    src="data:image/png;base64,">
                                <!-- Team Details -->
                            </a>
                    </div>
                    <div class="teamName">
                        <%=t.name%>
                            </p>
                    </div>
                </div>
                <div id="spriteRow">
                    <%t.pokemon.forEach(p=> { %>
                        <img height="45px" width="45px" src="<%=p.sprite%>">
                        <% }) %>
                </div>

                
        </div>

        <div id="deleteTeam">
            <form action="/teams/<%=t._id%>?_method=DELETE" method="post">
                <button type="submit" class="dt">Delete team</button>
            </form>
        </div>

        <% }) %>
    </div>

    <button class="createNewTeam"><a href="/teams/new">Create New Team</a></button>

    <%- include("../partials/footer")%>